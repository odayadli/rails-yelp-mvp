
  <% @restaurants.each do |restaurant| %>
 <h1> <%= restaurant.name %></h1>
  <h3><%= restaurant.address %></h3>
  <h4><%= restaurant.phone_number %></h4>
  <h4><%= restaurant.category %></h4>
  <h4><%= link_to 'add a review', new_restaurant_review_path(restaurant), class: "btn btn-primary" %></h4>
<% end %>


